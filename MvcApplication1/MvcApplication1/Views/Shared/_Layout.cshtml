<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Styles/bootstrap.min.css")
    @Styles.Render("~/Styles/demo.css")


    @Scripts.Render("~/Scripts/angular.min.js")
    @Scripts.Render("~/Scripts/ngStorage.min.js")
    @Scripts.Render("~/Scripts/lovefield.min.js")

    @Scripts.Render("~/Scripts/demo.js")
</head>
<body ng-app="myApp">
    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>
